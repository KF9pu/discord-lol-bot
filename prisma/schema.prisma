datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model user {
  user_id         BigInt     @id @unique
  guild_id        BigInt
  victories       Int     @default(0)
  defeats         Int     @default(0)
  last_game_date  Int
}

